#: ajax/clipboard.php:48
#, php-format
msgid "Could not remove '%s'"
msgstr "No se ha podido eliminar '%s'"

#: ajax/clipboard.php:55
#, php-format
msgid "Could not move '%s'"
msgstr "No se ha podido mover '%s'"

#: ajax/clipboard.php:59
#, php-format
msgid "Could not copy '%s'"
msgstr "No se ha podido copiar '%s'"

#: js/clipboard.js:11
msgid "Cut"
msgstr "Cortar"

#: js/clipboard.js:19
msgid "Copy"
msgstr "Copiar"

#: js/clipboard.js:26 js/clipboard.js:165
msgid "Paste"
msgstr "Pegar"

#: js/clipboard.js:63
msgid "Unable to paste: the files come from this directory."
msgstr "No se ha podido pegar: los archivos vienen de esta carpeta."

#: js/clipboard.js:64
msgid ""
"Unable to paste: you do not have the permissions to create files in this "
"directory."
msgstr ""
"No se ha podido copiar: no tienes permisos para crear archivos en esta "
"carpeta."

#: js/clipboard.js:65
msgid "Paste %n item"
msgid_plural "Paste %n items"
msgstr[0] "Pegar %n objeto"
msgstr[1] "Pegar %n objetos"

#: js/clipboard.js:131
msgid "An error occurred during the move."
msgstr "Ha ocurrido un error al mover."

#: js/clipboard.js:132
msgid "An error occurred during the copy."
msgstr "Ha ocurrido un error al copiar."

#: js/clipboard.js:153
msgid "Processing. Leaving the page now will interrupt the move."
msgstr ""
"Procesando. Abandonar la página ahora interrumpirá la acción de mover."

#: js/clipboard.js:154
msgid "Processing. Leaving the page now will interrupt the copy."
msgstr "Procesando. Abandonar la página ahora interrumpirá la copia."

#: js/clipboard.js:164
msgid ""
"The contents of the clipboard is in conflicts with elements already present "
"in this directory. Do you want to replace them ?"
msgstr ""
"Los contenidos del portapapeles está en conflicto con elementos ya "
"presentes en esta carpeta. ¿Quieres reemplazarlos?"
